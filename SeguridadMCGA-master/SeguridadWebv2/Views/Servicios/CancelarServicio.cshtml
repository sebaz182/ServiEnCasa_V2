@model  SeguridadWebv2.Models.App.Servicios
@{
    ViewBag.Title = "Cancelar Servicio";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}
<h2>Cancelar Servicio</h2>
<hr />
<h3>¿Esta seguro que desea cancelar el servicio?</h3>
<br />
<h3>@Html.DisplayFor(model => model.Presupuestos.Solicitudes.Desc_Solicitud)</h3>
<br />
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-group">
        <input type="submit" value="Cancelar Servicio" class="btn btn-danger" />
        @Html.ActionLink("Salir", "MisServicios", "Servicios", null, new { @class = "btn btn-default" })
    </div>
}
